{
  "name": "vite2.0",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "run:static-serer": "node ./package/server/index.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:push": "bash deploy.sh",
    "commit": "git-cz",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "homepage": "https://bhabgs.github.io/vite-vui-docs/",
  "repository": "https://github.com/bhabgs/vite-vui",
  "workspaces": [
    "package/*"
  ],
  "useWorkspaces": true,
  "dependencies": {
    "vue": "^3.0.11"
  },
  "private": true,
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.3",
    "@vue/compiler-sfc": "^3.0.5",
    "ant-design-vue": "2.2.2",
    "axios": "^0.21.1",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^6.2.0",
    "less": "^4.1.1",
    "patch-vue-directive-ssr": "^0.0.1",
    "typescript": "^4.1.3",
    "vite": "^2.4.4",
    "vitepress": "^0.15.6",
    "vue-router": "^4.0.4",
    "@antv/x6": "^1.24.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
